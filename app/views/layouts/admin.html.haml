  %body.index
    / begin accesibility skip to nav skip content
    =render "layouts/admin_nav"
    #page
      -if flash[:notice]
        .alert.alert-info
          %button.close{type:"button", data: {dismiss: "alert"}}
            &times;
          =flash[:notice]
      -if flash[:error]
        .alert.alert-error
          %button.close{type:"button", data: {dismiss: "alert"}}
            &times;
          =flash[:error]
      =flash[:alert]
      =yield

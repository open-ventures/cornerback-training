%body
  / begin #page - the container for everything but header
  #page.user_profile
    .container.visible-phone
      %p.right
        %a.btn.btn-primary.scrollto{:href => "#why-join"}
          Why Join? &nbsp;
          %i.icon-info-sign
    .hero-unit
      .container
        %h6 Player Profile
        %ul.breadcrumb
          %li
            %a{"data-toggle" => "tab", :href => "#settings"} Account Settings
            %span.divider /
          %li.active
            %a{"data-toggle" => "tab", :href => "#profile"} Cornerback Profile
            %span.divider /
          %li
            %a{"data-toggle" => "tab", :href => "#testing"} Performance Testing
            %span.divider /
          %li
            %a{"data-toggle" => "tab", :href => "#survey"} Cornerback Survey
      / close container
    / close hero-unit
    #main-content.container.clearfix
      .row-fluid
        .span12
          #myTabContent.tab-content
            / begin myTabContent
            / **************** BEGIN RESUME ****************
            #settings.tab-pane.fade
              / Begin tab-pane resumes
              %button.btn.btn-med{:type => "submit", :value => ""} Edit
              %button.btn.btn-med{:type => "submit", :value => ""} Update
              %form#accountSettings{:action => "#.php", :method => "post"}
                %fieldset
                  %h5 Account Settings
                  .control-group
                    .control-group
                      %label.required.control-label{:for => "firstName"} First Name
                      .controls
                        %input#firstName.span3{:name => "firstName", :type => "text"}/
                    .control-group
                      %label.required.control-label{:for => "lastName"} Last Name
                      .controls
                        %input#lastName.span3{:name => "lastName", :type => "text"}/
                    .control-group
                      %label.required.control-label{:for => "email"} E-mail
                      .controls
                        %input#email.span3{:name => "email", :type => "email"}/
                    %h5 Change Your Password
                    .control-group
                      %label.required.control-label{:for => "newPassword"} Password
                      .controls
                        %input#newPassword.span3{:name => "newPassword", :type => "text"}/
                    .control-group
                      %label.required.control-label{:for => "confirmPassword"} Confirm Password
                      .controls
                        %input#confirmPassword.span3{:name => "confirmPassword", :type => "text"}/
                    .control-group
                      %label.required.control-label{:for => "currentPassword"} Current Password
                      .controls
                        %input#currentPassword.span3{:name => "currentPassword", :type => "text"}/
                    %br/
                    %br/
                    %span Unhappy?
                    %a{:href => "#"} Cancel my account
            / close tab-pane fade in active
            / END RESUME

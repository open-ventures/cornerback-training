      %ul.breadcrumb
        %li.active
          %a{"data-toggle" => "tab", :href => "#mon"} M
          %span.divider /
        %li
          %a{"data-toggle" => "tab", :href => "#tue"} Tu
          %span.divider /
        %li
          %a{"data-toggle" => "tab", :href => "#wed"} W
          %span.divider /

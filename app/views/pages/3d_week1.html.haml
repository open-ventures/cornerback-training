        #myTabContent.tab-content
          / begin myTabContent
          / **************** BEGIN MONDAY ****************
          #mon.tab-pane.fade.in.active
            / Begin tab-pane Mon
            %h2 MONDAY
            %ul.nav.nav-pills
              %li.active
                %a{:href => "#"} Speed training
              %li.active
                %a{:href => "#"} lowerbody strength (20RM)
            .groove-border-eee
            %h4 WORKOUT I:  SPEED TRAINING
            / INSERT RENDER PARTIAL speedTraining_warmUp
            =render partial: "speedTraining_warmUp"
            %hr/
            %span.label.label-inverse WORKOUT
            %h5 Strides
            .row-fluid.exercises.main-workout
              .span12
                %h5 2 x 1 mile, walk turns, stride straights @ 80% speed (no rest)
                %ul
                  / INSERT RENDER PARTIAL notes_strides80percent
                  =render partial: "notes_strides80percent"
            %hr/
            / INSERT RENDER PARTIAL speedTraining_coolDown
            =render partial: "speedTraining_coolDown"
            .wo_break Minimum 1-hour training Break
            %h4 WORKOUT II:  LOWERBODY STRENGTH TRAINING
            / INSERT RENDER PARTIAL lowerbodyStrength_warmUp
            =render partial: "lowerbodyStrength_warmUp"
            %hr/
            .row-fluid
              %span.label.label-inverse WORKOUT
              %h5 Lowerbody Strength Training
              .row-fluid
                .span12
                  %h5 Weights: 20RM, 60-75sec rest/set
                  %table.table.table-striped
                    %tr
                      %th Exercise
                      %th Intensity
                      %th Tempo
                      %th Volume
                      %th Rest
                    %tr
                      %td
                        %a{:href => "#"} Single leg press
                      %td 20RM
                      %td 3-0-1-1
                      %td 3 x 20
                      %td :60-75
                    %tr
                      %td
                        %a{:href => "#"} stability ball Hamstring curls

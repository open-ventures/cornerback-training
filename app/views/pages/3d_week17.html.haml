            %ul.nav.nav-pills
              %li.active
                %a{:href => "#"} Speed training
              %li.active
                %a{:href => "#"} lowerbody strength (20RM)
            .groove-border-eee
            %h4 WORKOUT I:  SPEED TRAINING
            / INSERT RENDER PARTIAL speedTraining_warmUp
            =render partial: "speedTraining_warmUp"
            %hr/
            %span.label.label-inverse WORKOUT
            %h5 Advanced Speed Training w/ weights
            .row-fluid.exercises.main-workout
              .row-fluid
                .span6
                  %h5 1. Tuck Jumps w/ 7.5% BW, 5 x 25 jumps
                  %ul
                    / INSERT RENDER PARTIAL notes_tuckJumps
                    =render partial: "notes_tuckJumps"
                .span6
                  %h5 2. Single-leg stair bounds w/ 7.5% BW, 5x
                  %ul
                    / INSERT RENDER PARTIAL notes_singleLegStairBounds
                    =render partial: "notes_singleLegStairBounds"
              .row-fluid
                .span6
                  %h5 3. Broad Jumps w/ 7.5% BW, 20x
                  %ul
                    / INSERT RENDER PARTIAL notes_broadJump
                    =render partial: "notes_broadJump"
                .span6
                  %h5 4. 20-yard Power Sled 8x
                  %ul
                    / INSERT RENDER PARTIAL notes_20ydPowerSled
                    =render partial: "notes_20ydPowerSled"

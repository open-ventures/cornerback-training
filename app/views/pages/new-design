      $(window).load(function(){
        $('[data-spy="scroll"]').each(function () {
          var t = $(this);
          t.scrollspy();
          $(window).resize(function(){
            t.scrollspy('refresh');
            $(this).trigger('scroll');
          });
        });
      });
      

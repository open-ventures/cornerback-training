  / CANVAS
  .l-canvas.type_wide.headertype_extended
    .l-canvas-h.workout

      .l-main
        .l-main-h

          .l-submain.for_pagehead.color_alternate
            .l-submain-h.g-html.i-cf
              .w-pagehead
                %ul.program-guide 
                  %li 
                    %a{href: "/cornerback_training_goals"}Training Goals
                  %li 
                    %a{href: "/cornerback_training_phases"}Phases I, II, III
                  %li 
                    %a{href: "/cornerback_training_weekly_breakdown"} Weekly Breakdown
                  %li 
                    %a{href: "/cornerback_training_schedule"}Schedule
                  %li 
                    %a{href: "/cornerback_training_workout_structure"}Workout Structure

          .l-submain.workout.program-guide.schedule
            .l-submain-h.g-html.i-cf
              .l-content
                .l-content-h
                  .w-blogpost-text
                    %h2.section-title PHASE 1: 6 DAYS ON, 1 DAY OFF.
                    
                    .g-cols
                      .one-third
                        .g-cols
                          .one-half
                            .label DAY 1
                            %h3 MON
                            .label.label-info Speed Training
                            .label.label-info LB Strength
                          .one-half
                            .label DAY 2
                            %h3 TUE
                            .label.label-warning Footwork Agility
                            .label.label-warning Cornerback Drills
                            .label.label-warning UB Strength
                      .one-third
                        .g-cols
                          .one-half
                            .label DAY 3
                            %h3 WED
                            .label.label-danger Core Strength
                            .label.label-danger Flexibility
                          .one-half
                            .label DAY 4
                            %h3 THU
                            .label.label-info Speed Training
                            .label.label-info LB Strength
                      .one-third
                        .g-cols
                          .one-half
                            .label DAY 5
                            %h3 FRI
                            .label.label-warning Footwork Agility
                            .label.label-warning Cornerback Drills
                            .label.label-warning UB Strength
                          .one-half
                            .label DAY 6
                            %h3 SAT
                            .label.label-danger Core Strength
                            .label.label-danger Flexibility


            
                    %br
                    .hr.lg
                    %br

                    .g-cols
                      .one-third
                        %h6 'Speed & LB Strength'
                        %h3 MON  THU
                        %h5 SPEED TRAINING.
                        %ol
                          %li Speed and lower-body strength training.
                          %li Make sure you focus on performing speed drills with Great sprint technique.
                      .one-third
                        %h6 'Footwork & UB Strength'
                        %h3 TUE  FRI
                        %h5 FOOTWORK, AGILITY, DRILLS
                        %ol
                          %li Footwork and agility, cornerback drills, and upper-body strength.
                      .one-third
                        %h6 'Core & Flexibility'
                        %h3 WED  SAT
                        %h5 FLEXIBILITY TRAINING
                        %ol
                          %li Flexibility and core strength.
                          %li It is recommended that you participate in a progressive yoga program. If you can't, a round of foam rolling, followed by static stretching will suffice
                          %li Make sure you start off in a beginners class.
                    
                                        
                                       
                        
                  
                .w-bloglist
                  %h4.w-bloglist-title Comments
                  =render partial: "pages/disqus"
                      
